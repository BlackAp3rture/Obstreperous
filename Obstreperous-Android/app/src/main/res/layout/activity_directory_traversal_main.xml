<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.obstreperous.DirectoryTraversal.DirectoryTraversalMain"
    android:background="#FFFFFF">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Directory Traversal"
        android:id="@+id/textView10"
        android:textColor="#001e79"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textStyle="bold"
        android:textAlignment="center"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:paddingTop="16dp"
        android:textSize="27dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="A Directory Traversal vulnerability is when an application does not care to take the action of verifying an appropriate file path that gets parsed allowing an attack to navigate to a different directory than the developer intended, usually resulting in an attacker gaining read/write privileges inside the internal storage."
        android:id="@+id/textView21"
        android:layout_below="@+id/textView10"
        android:layout_centerHorizontal="true"
        android:padding="16dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:textColor="#000000"
        android:textAlignment="center"
        android:textSize="17dp" />
</RelativeLayout>
